module tug::boat;

use sui::coin::{Self};
use sui::url::{Url, Self};

public struct BOAT has drop {}

fun init(witness: BOAT, ctx: &mut TxContext) {

    let symbol = b"TUGBOAT";
    let name = b"TUGBOAT";
    let url: Url = url::new_unsafe_from_bytes(b"data:image/png;base64,");
    let description = b"Tugboat.fi token";

    let (mut treasury, metadata) = coin::create_currency(witness, 9, symbol,name, 
    description, option::some(url), ctx);
    let amount = 100_000_000 * 1_000_000_000;
    coin::mint_and_transfer(&mut treasury, amount, ctx.sender(), ctx);
    transfer::public_freeze_object(metadata);
    transfer::public_transfer(treasury, @0x0);
}